START;
function plus;
    z=x+y;
    print(x, y, z);
end;
function minus;
    z=x-y;
    print(x, y, z);
end;
function multiply;
    z=x*y;
    print (x, y, z);
end;
function divide;
    z=x/y;
    print(x, y, z);
end;
var x;
var y;
var z;
x = 10;
y = 2;
call plus;
call minus;
call multiply;
call divide;
FINISH;